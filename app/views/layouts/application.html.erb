<!DOCTYPE html>
<html>
<head>
  <title>Swagrid</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="container">
  <div class="row">
    <div class="col-md-5">
    </div>
    <div class="col-md-4">
      <%= link_to "SWAGRID", "/" %>
      <!-- change to new_session_paths -->
    </div>
    <div class="col-md-1">
      <%= link_to "LOGIN", "/", class: "btn btn-default" %>
    </div>
    <div class="col-md-1">
      <%= link_to "LOGOUT", "/", class: "btn btn-default" %>
    </div>
    <div class="col-md-1">
      <%= link_to "CART", "/", class: "btn btn-default" %>
    </div>
  </div>

  <%= yield %>
</div>
</body>
</html>
