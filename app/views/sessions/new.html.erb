<h1>Log In</h1><br>
<div class="form-group">
  <div class="center-div">

    <%= form_for(:session_data, url: sessions_path) do |f| %>
      <div class="form-group">
        <%= f.label :email %>
        <%= f.email_field :email, class: "form-control" %>

        <%= f.label :password %>
        <%= f.password_field :password, class: "form-control" %>
      </div>
      <%= f.submit "Login", class:"btn btn-info" %>
    <% end %>

  </div>
</form>
