<h1>Log In</h1><br>
<div class="form-group">
  <div class="center-div">
    <%= form_tag sessions_path do %>
      <%= label_tag :email %><br>
      <%= email_field_tag :email, params[:username],:class => "form-control", placeholder: "Email Address" %><br><br>
      <%= label_tag :password %><br>
      <%= password_field_tag :password, "", :class => "form-control", placeholder: "Password" %><br><br>
      <%= submit_tag "Log in", :class => 'btn btn-default' %>
    <% end %>
  </div>
</form>
