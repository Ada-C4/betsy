<div class="container">
  <%= render 'layouts/navbar' %>
  <div class="row" id="merchant_home">
    <div class="col-lg-12">
      <h4>Welcome, <%= @current_merchant.username %></h4>

      <%= link_to "View Your Products", merchant_products_path(@current_merchant.id) %>
      <br>
      <%= link_to "View Your Orders", merchant_orders_path(@current_merchant.id) %>
      <br>
      <%= link_to "Log Out", logout_path, method: :delete %>
    </div>
  </div>
</div>
