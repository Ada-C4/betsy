<h1> dropdown merchants goes here </h1>

<div class="row">
  <div class="col-md-12 text-center">
    <h1> All Products </h1>
  </div>
</div>


<table class="table table-hover">
    <tbody>
      <% @products.each do |product| %>
        <tr>
          <td> <%= link_to product.name, product %></td>
          <td> <%= product.price %></td>
          <td> <%= image_tag(product.photo_url) %></td>
        </tr>
        <% end %>
    </tbody>
</table>
